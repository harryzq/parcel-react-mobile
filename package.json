{
  "name": "parcelvue",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "parcel:dev": "parcel index.html --open",
    "parcel-dev-sh": "bash dev.sh",
    "parcel:prod": "parcel build index.html --out-dir build/dist --detailed-report",
    "parcel-prod-sh": "bash prod.sh",
    "start": "npm-run-all parcel-dev-sh parcel:dev",
    "build": "npm-run-all parcel-prod-sh parcel:prod"
  },
  "alias": {
    "@":"./src",
    "pages": "./src/pages",
    "assets": "./src/assets"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@loadable/component": "^5.11.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.24.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "assets": "^3.0.1",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
